functionGetCookie(cKey){
varallcookies=document.cookie;
varcookies=allcookies.split(";");
for(vari=0;i<cookies.length;i++){
varkeyValues=cookies[i].split("=");
if(keyValues[0]==cKey){
returnunescape(keyValues[1]);
}
}
return"";
}

functionSetCookie(cKey,cValue)//name,pwd
{
document.cookie=cKey+'='+escape(cValue);
}



varbase_time=3599

functiontime_display(){

